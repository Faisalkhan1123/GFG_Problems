#include <iostream>
using namespace std;

	//code


int main() {
	//code
	int t;
	cin>>t;
	while(t--){
	    long int n,d;
	    cin>>n>>d;
	    int a[n], i;
	    for( i=0; i<n; i++)
	        cin>>a[i];
	    
	    for(i=d; i<n; i++)
	        cout<<a[i]<<" ";
	        
	    
	    for( i=0; i<d;i++)
	        cout<<a[i]<<" ";
	        
	        cout<<endl;
	    
	    
	}
	return 0;
}
	
