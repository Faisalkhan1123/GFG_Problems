#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	 int odd=0;
	 
	  int a[26]={0};
	while(t--){
	    string s;
	    cin>>s;
	   
	   
	    for(int i=0; i<s.length(); i++){
	        a [s[i]-'a']++;
	        
	    }
	    for(int i=0; i<26;i++){
	        if(a[i]%2!=0)
	        odd++;
	    }
	    
	    if(a[i]%2==0){
	    if(odd==0)
	    cout<<"Yes";
	    else
	    cout<<"No";
	    }
	    else{
	        if(odd==1)
	        cout<<"Yes";
	        else 
	        cout<<"No";}
	        
	    }
	    
	}
	
	
	return 0;
}
